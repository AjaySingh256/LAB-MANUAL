 #include <stdio.h>
int main()
{
    int n;
    printf("Enter a number :");
 scanf("%d", &n);
    if (n % 5 == 0 && n % 3 == 0)
    {
        printf("It is a divisibal 5 and 3");
    }
    else
    {
        printf("It is not a divisibal 5 and 3");
    }
    return 0;
}
// ***OUTPUT***
// Enter a number :15
// It is a divisibal 5 and 3
