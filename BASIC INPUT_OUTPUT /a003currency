// CURRENCY
#include<stdio.h>
int main()
{
 int rs, a,b,c,d,e,f,g,h,i;
 printf("Enter Amount in Rupee:");
 scanf("%d",&rs);
   while(rs>=500)
 {
 a= rs/500;
 rs= rs%500;
    printf("\n The no of five hundred:%d",a);
    break;
    }
 while(rs>=200)
    {
    b= rs/200;
    rs= rs%200;
    printf("\n The no of Two hundred:%d",b);
    break;
    }
 while(rs>=100)
    {
    c= rs/100;
    rs= rs%100;
    printf("\n The no of One hundred:%d",c);
    break;
    }
 while(rs>=50)
    {
    d= rs/50;
    rs= rs%50;
    printf("\n The no of fity :%d",d);
    break;
    }
 while(rs>=20)
    {
    e= rs/20;
    rs= rs%20;
    printf("\n The no of twenty :%d",e);
    break;
    }
 while(rs>=10)
    {
    f= rs/10;
    rs= rs%10;
    printf("\n The no of Ten :%d",f);
    break;
    }
 while(rs>=5)
    {
    g= rs/5;
    rs= rs%5;
    printf("\n The no of five :%d",g);
    break;
    }
 while(rs>=2)
    {
    h= rs/2;
    rs= rs%2;
    printf("\n The no of Two:%d",h);
    break;
    }
 while(rs>=1)
    {
    i= rs/1;
    rs= rs%1;
    printf("\n The no of one:%d",i);
    break;
    }
    return 0;
}
// ***OUTPUT***
//  Enter Amount in Rupee:1858
//  The no of five hundred:3       
//  The no of Two hundred:1        
//  The no of One hundred:1        
//  The no of fity :1
//  The no of five :1
//  The no of Two:1
//  The no of one:1
