#include<stdio.h>
int main()
{
    int n, i;
    printf("Enter the number you want a Table:");
    scanf("%d",&n);

   for(i=10; i>=1; i--)
   // for(i=1 ; i<=10; i++)
{
    printf("%d * %d = %d\n",n,i,n*i);
}
return 0;
}
// ***OUTPUT***
// Enter the number you want a Table:10
// 10 * 10 = 100
// 10 * 9 = 90
// 10 * 8 = 80
// 10 * 7 = 70
// 10 * 6 = 60
// 10 * 5 = 50
// 10 * 4 = 40
// 10 * 3 = 30
// 10 * 2 = 20
// 10 * 1 = 10
